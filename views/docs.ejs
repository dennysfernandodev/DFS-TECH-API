    if (rota === "ia_imagem") return withKey(`/api/ai/imagem/imagemAi?query=${encodeURIComponent(params.query || "")}`);
    if (rota === "ia_hd") return `/api/ai/hd/imagem?url=${encodeURIComponent(params.url || "")}`;

    if (rota === "canva_bemvindo") return withKey(`/api/canva/bemVindo?titulo=${encodeURIComponent(params.titulo || "")}&avatar=${encodeURIComponent(params.avatar || "")}&fundo=${encodeURIComponent(params.fundo || "")}&desc=${encodeURIComponent(params.desc || "")}&nome=${encodeURIComponent(params.nome || "")}`);
