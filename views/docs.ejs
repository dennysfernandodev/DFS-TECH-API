  }

  function getFields() {
    const categoria = elCategoria.value;
    const rota = elRota.value;
    if (categoria === "puxadas") return fieldsByTipo[rota] || [];
    if (categoria === "dadosPessoais") return rotaToFieldsDadosPessoais[rota] || [];
    if (categoria === "utilitarios") return rotaToFieldsUtil[rota] || [];
    return rotaToFieldsApi[rota] || [];
  }

  function renderFields() {
    const fields = getFields();
    elParams.innerHTML = "";
    fields.forEach((f) => {
      const wrap = document.createElement("div");
      const label = document.createElement("label");
      label.className = "block text-sm mb-1";
      label.textContent = f.label;
      const input = document.createElement("input");
      input.className = "form-control !bg-slate-900 !text-white !border-slate-700";
      input.placeholder = f.label;
      input.dataset.param = f.key;
      wrap.appendChild(label);
      wrap.appendChild(input);
      elParams.appendChild(wrap);
    });
  }

  function collectParams() {
    const out = {};
    elParams.querySelectorAll("input[data-param]").forEach((i) => {
      const val = (i.value || "").trim();
      if (val) out[i.dataset.param] = val;
    });
    return out;
  }

  function buildRequestUrl() {
    const categoria = elCategoria.value;
    const rota = elRota.value;
    const params = collectParams();
    const q = new URLSearchParams();
    Object.entries(params).forEach(([k, v]) => q.set(k, v));
    q.set("apikey", painelApiKey);
    q.set("formato", "texto");

    if (categoria === "puxadas") return `/api/puxadas/consulta/${encodeURIComponent(rota)}?${q.toString()}`;
    if (categoria === "dadosPessoais") return `/api/dados-pessoais/${encodeURIComponent(rota)}?${q.toString()}`;
    if (categoria === "utilitarios") {
      if (rota === "consulta_cep") return `/api/consulta/cep/${encodeURIComponent(params.cep || "")}?apikey=${encodeURIComponent(painelApiKey)}&formato=texto`;
      if (rota === "consulta_ddd") return `/api/consulta/ddd/${encodeURIComponent(params.ddd || "")}?apikey=${encodeURIComponent(painelApiKey)}&formato=texto`;
      if (rota === "consulta_clima") return `/api/consulta/clima/aeroporto/${encodeURIComponent(params.codigoICAO || "")}?apikey=${encodeURIComponent(painelApiKey)}&formato=texto`;
      return `/api/gerar-cpf?apikey=${encodeURIComponent(painelApiKey)}&formato=texto`;
    }

    const withKey = (base) => `${base}${base.includes("?") ? "&" : "?"}apikey=${encodeURIComponent(painelApiKey)}`;

    if (rota === "yt_public_search") return `/api/public/youtube/search?nome=${encodeURIComponent(params.nome || "")}`;
    if (rota === "yt_public_video") return `/api/public/youtube/video?nome=${encodeURIComponent(params.nome || "")}`;
    if (rota === "yt_public_download") return `/api/public/youtube/download?nome=${encodeURIComponent(params.nome || "")}`;
    if (rota === "yt_public_download_mp3") return `/api/public/youtube/download-mp3?nome=${encodeURIComponent(params.nome || "")}`;
