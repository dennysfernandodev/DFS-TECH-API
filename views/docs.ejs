  function buildRouteOptions() {
    const categoria = elCategoria.value;
    const routes = categorias[categoria] || [];
    elRota.innerHTML = "";
    routes.forEach((r) => {
      const opt = document.createElement("option");
      opt.value = r;
      opt.textContent = r;
      elRota.appendChild(opt);
    });
