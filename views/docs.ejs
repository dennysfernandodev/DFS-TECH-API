    if (rota === "download_mediafire") return withKey(`/api/download/mediafire?url=${encodeURIComponent(params.url || "")}`);

    if (rota === "pesquisa_youtube") return withKey(`/api/pesquisa/youtube?nome=${encodeURIComponent(params.nome || "")}`);
    if (rota === "pesquisa_anime") return `/api/pesquisa/anime?nome=${encodeURIComponent(params.nome || "")}`;
    if (rota === "pesquisa_playstore") return withKey(`/api/pesquisa/playstore?nome=${encodeURIComponent(params.nome || "")}`);
    if (rota === "pesquisa_gimage") return withKey(`/api/pesquisa/gimage?q=${encodeURIComponent(params.q || "")}`);
    if (rota === "pesquisa_pinterestgif") return withKey(`/api/pesquisa/pinterestgif?nome=${encodeURIComponent(params.nome || "")}`);

    if (rota === "ia_chatgpt") return `/api/ai/texto/chatgpt?query=${encodeURIComponent(params.query || "")}`;
