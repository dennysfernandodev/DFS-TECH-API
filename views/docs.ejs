  async function consultar() {
    function objectToText(data, indent = 0) {
      const pad = "  ".repeat(indent);
      if (data === null || data === undefined) return `${pad}-`;
      if (typeof data !== "object") return `${pad}${String(data)}`;
      if (Array.isArray(data)) {
        if (!data.length) return `${pad}(sem resultados)`;
        return data
          .map((item, index) => {
            const head = `${pad}${index + 1}.`;
            const body = objectToText(item, indent + 1);
            return `${head}\n${body}`;
          })
          .join("\n");
      }
      const entries = Object.entries(data);
      if (!entries.length) return `${pad}(vazio)`;
      return entries
        .map(([k, v]) => {
          if (v && typeof v === "object") {
            return `${pad}${k}:\n${objectToText(v, indent + 1)}`;
          }
          return `${pad}${k}: ${v === null || v === undefined ? "-" : String(v)}`;
        })
        .join("\n");
    }

    try {
      const url = buildRequestUrl();
      elResultado.textContent = "Consultando...";
      if (painelObjectUrl) {
        URL.revokeObjectURL(painelObjectUrl);
        painelObjectUrl = null;
      }
      elMidia.style.display = "none";
      elMidia.innerHTML = "";
      const res = await fetch(url, { method: "GET", credentials: "same-origin" });
      const contentType = (res.headers.get("content-type") || "").toLowerCase();
      const isTextLike = contentType.includes("json") || contentType.startsWith("text/") || contentType.includes("javascript");

      if (isTextLike) {
        const text = await res.text();
        let parsed = text;
        try { parsed = JSON.parse(text); } catch (_) {}
        elResultado.textContent = typeof parsed === "string" ? parsed : objectToText(parsed);
      } else {
        const blob = await res.blob();
        painelObjectUrl = URL.createObjectURL(blob);
        elResultado.textContent = `Resposta de mídia/arquivo: ${contentType || "binário"} (${blob.size} bytes)`;

        if (contentType.startsWith("image/")) {
          elMidia.innerHTML = `<img src="${painelObjectUrl}" alt="preview" style="max-width:100%;border-radius:8px;border:1px solid #334155;" />`;
          elMidia.style.display = "block";
        } else if (contentType.startsWith("audio/")) {
          elMidia.innerHTML = `<audio controls style="width:100%;" src="${painelObjectUrl}"></audio>`;
          elMidia.style.display = "block";
        } else if (contentType.startsWith("video/")) {
          elMidia.innerHTML = `<video controls style="max-width:100%;border-radius:8px;border:1px solid #334155;" src="${painelObjectUrl}"></video>`;
          elMidia.style.display = "block";
        } else {
          elMidia.innerHTML = `<a href="${url}" target="_blank" rel="noopener" class="btn btn-primary">ABRIR/BAIXAR ARQUIVO</a>`;
          elMidia.style.display = "block";
        }
      }
      if (!res.ok) {
        elResultado.textContent = `[HTTP ${res.status}] ` + elResultado.textContent;
      }
    } catch (e) {
      elResultado.textContent = `Erro: ${e.message}`;
    }
  }

  elCategoria.addEventListener("change", buildRouteOptions);
  elRota.addEventListener("change", renderFields);
  elConsultar.addEventListener("click", consultar);
  elLimpar.addEventListener("click", () => {
    elParams.querySelectorAll("input[data-param]").forEach((i) => (i.value = ""));
    elResultado.textContent = "";
    if (painelObjectUrl) {
      URL.revokeObjectURL(painelObjectUrl);
      painelObjectUrl = null;
    }
    elMidia.style.display = "none";
    elMidia.innerHTML = "";
  });

  elCopiar.addEventListener("click", async () => {
    const texto = (elResultado.textContent || "").trim();
    if (!texto) return;
    try { await navigator.clipboard.writeText(texto); } catch (_) {}
  });
  buildRouteOptions();
})();
